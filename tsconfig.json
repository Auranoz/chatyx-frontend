{
    "compilerOptions": {
        "jsx": "react",
        "baseUrl": "./src",
        "target": "es5",
        "module": "esnext",
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "removeComments": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noErrorTruncation": true,
        "allowSyntheticDefaultImports": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "paths": {
            "@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"]
        }
    }
}
